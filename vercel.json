{
    "rewrites": [
      { "source": "/api/:path*", "destination": "/api/:path*" }
    ],
    "builds": [
      { "src": "Frontend/package.json", "use": "@vercel/node" },
      { "src": "Backend/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "Backend/app.js" },
      { "src": "/(.*)", "dest": "Frontend/$1" }
    ]
  }